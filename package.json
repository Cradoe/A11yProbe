{
  "name": "a11yprobe",
  "version": "1.0.0",
  "description": "A package for checking the accessibility of a website by automatically retrieving all subpages and running accessibility tests with the WCAG 2.1 AA standard.",
  "main": "lib/index.js",
  "type": "module", 
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Cradoe/A11yProbe"
  },
  "keywords": [
    "accessibility",
    "pa11y",
    "test",
    "web"
  ],
  "homepage": "https://github.com/Cradoe/A11yProbe",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node lib/index.js",
    "publish:npm": "rm -rf lib && npm run build && npm publish --access public"
  },
  "author": "Ismail Obadimu",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.4",
    "pa11y": "^6.2.3",
    "puppeteer": "^19.8.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.1",
    "@types/pa11y": "^5.3.5",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.4.4"
  }
}
