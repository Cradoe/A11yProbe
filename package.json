{
  "name": "a11yprobe",
  "version": "1.0.13",
  "description": "A package for checking the accessibility of a website by automatically retrieving all subpages and running accessibility tests with the WCAG 2.1 AA standard.",
  "main": "lib/index.js",
  "private": false,
  "type": "module",
  "bin": {
    "a11yprobe": "lib/index.js"
  },
  "keywords": [
    "accessibility",
    "testing",
    "a11y"
  ],
  "homepage": "https://github.com/Cradoe/A11yProbe",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "publish:npm": "rm -rf lib && npm run build && npm publish",
    "publish:yarn": "rm -rf lib && npm run build && yarn publish",
    "prepare": "node ./scripts/prepublish.js",
    "postpublish": "node ./scripts/postpublish.js"
  },
  "author": "Ismail Obadimu",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.4",
    "cli-progress": "^3.12.0",
    "pa11y": "^6.2.3",
    "puppeteer": "^19.8.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.1",
    "@types/pa11y": "^5.3.5",
    "@types/progress": "^2.0.5",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.4.4",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cradoe/a11yprobe.git"
  },
  "bugs": {
    "url": "https://github.com/cradoe/a11yprobe/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}